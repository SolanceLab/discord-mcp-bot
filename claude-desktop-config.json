{
  "mcpServers": {
    "discord-mcp-bot": {
      "command": "node",
      "args": ["/path/to/your/discord-mcp-bot/dist/mcp-server.js"],
      "env": {
        "DISCORD_BOT_TOKEN": "your_discord_bot_token",
        "ANTHROPIC_API_KEY": "your_anthropic_api_key",
        "OWNER_USER_ID": "your_discord_user_id"
      }
    }
  }
}
